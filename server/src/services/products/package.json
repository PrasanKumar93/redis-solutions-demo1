{
  "name": "products_service",
  "version": "1.0.0",
  "scripts": {
    "postinstall": "cd ../../common/ && npm install",
    "prisma-types": "npx prisma generate --schema=\"prisma-generated/schema.prisma\"",
    "build": "npm run prisma-types && npx tsc",
    "start": "npm run build && concurrently -k \"npx tsc --watch\" \"nodemon  dist/services/products/src/index.js\""
  },
  "dependencies": {
    "@prisma/client": "^4.16.2",
    "crypto": "^1.0.1",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "mongodb": "^4.8.0",
    "prisma": "^4.16.2",
    "redis": "^4.6.7",
    "redis-om": "^0.4.0",
    "yup": "^1.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/mongodb": "^4.0.7",
    "@types/node": "^18.0.6",
    "concurrently": "^7.3.0",
    "nodemon": "^2.0.19",
    "typescript": "^4.7.4"
  }
}
