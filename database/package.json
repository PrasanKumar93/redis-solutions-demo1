{
  "name": "database",
  "version": "1.0.0",
  "description": "Database seeder",
  "scripts": {
    "copy-js": "cp -r src/triggers dist/",
    "schema": "npx prisma generate",
    "build": "npm run schema && npx tsc && npm run copy-js",
    "start": "npx prisma db push  && npm run build &&  concurrently -k \"npx tsc\" \"node  dist/server.js\""
  },
  "dependencies": {
    "@prisma/client": "^4.16.2",
    "dotenv": "^16.0.3",
    "prisma": "^4.16.2",
    "redis": "^4.6.10"
  },
  "devDependencies": {
    "@types/node": "^20.3.3",
    "concurrently": "^8.2.0",
    "typescript": "^5.1.6"
  }
}